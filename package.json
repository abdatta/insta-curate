{
  "name": "insta-curate",
  "version": "1.0.0",
  "description": "Minimal Instagram Curator PWA",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"tsx watch src/main.ts\" \"npm run dev --prefix client\"",
    "dev:backend": "tsx watch src/main.ts",
    "dev:client": "npm run dev --prefix client",
    "build": "tsc",
    "start": "node dist/main.js",
    "login": "tsx src/curator/auth.ts",
    "curate": "tsx src/curator/runCuration.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "playwright": "^1.40.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/web-push": "^3.3.2",
    "concurrently": "^9.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
